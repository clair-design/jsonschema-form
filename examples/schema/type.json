{
  "title": "Different data types and formats",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "theme": {
      "type": "string",
      "format": "color",
      "default": "#ffcc00"
    },
    "logo": {
      "type": "string",
      "format": "image",
      "description": "400x400, within 1M"
    },
    "height": {
      "type": "integer",
      "minimum": 100,
      "maximum": 200,
      "description": "cm",
      "default": 170
    },
    "size": {
      "type": "string",
      "enum": [
        "XS",
        "S",
        "M",
        "L",
        "XL"
      ],
      "default": "M"
    },
    "city": {
      "type": "string",
      "options" : [
        { "value": "Beijing", "label": "北京" },
        { "value": "Chengdu", "label": "成都" },
        { "value": "Guangzhou", "label": "广州" },
        { "value": "Hangzhou", "label": "杭州" },
        { "value": "Nanjing", "label": "南京" },
        { "value": "Shanghai", "label": "上海" },
        { "value": "Shenzhen", "label": "深圳" },
        { "value": "Wuhan", "label": "武汉" },
        { "value": "Xi'an", "label": "西安" }
      ]
    },
    "subscribe": {
      "type": "boolean"
    }
  }
}
